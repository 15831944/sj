array_getAt(index'i32:)'char c{

		int32_t size;
		#retain(i32, size, size);
		uintptr_t data;
		#retain(ptr, data, data);

		if (index >= size || index < 0) {
			printf("getAt: out of bounds\n");
			exit(-1);
		}

		#type(t)* p = (#type(t)*)data;
		#return(t, p[index]);		
	
}c

array_isEqual(test'stack array_char:)'bool c{

		#include(<string.h>)

		if (size != test->size) {
			*_return = false;
		}

		*_return = memcmp((void*)data, (void*)test->data, size * sizeof(#type(t))) == 0;		
	
}c

array_isLessOrEqual(test'stack array_char:)'bool c{

		#include(<string.h>)

		*_return = memcmp((void*)data, (void*)test->data, (size < test->size ? size : test->size) * sizeof(#type(t))) <= 0;		
	
}c

string(count'i32=, data'stack array_char=)'stack string {
