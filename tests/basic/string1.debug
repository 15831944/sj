string_getAt(index'i32:)'char {
  array_getAt(
    parent: .parentthis.data,
    index'i32 : this.index
  )
}

string_isEqual(test'stack string:)'bool {
  array_isEqual(
    test'stack array_char : this.test
  )
}

string_isLessOrEqual(test'stack string:)'bool {
  array_isLessOrEqual(
    test'stack array_char : this.test
  )
}

